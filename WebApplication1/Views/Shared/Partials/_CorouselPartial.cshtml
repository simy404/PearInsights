@model List<WebApplication1.Models.Corousel>



<div id="carouselExampleControls" class="carousel slide  bg-primary text-white  " data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item">
            <img class="d-block img-fluid" style="width: 100%; height: 650px;overflow: hidden;" src="@Model[0].url" alt="First slide">
        </div>
        <div class="carousel-item active">
            <img class="d-block img-fluid" style="width: 100%; height: 650px;overflow: hidden;" src="@Model[1].url" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block img-fluid" style="width: 100%; height: 650px;overflow: hidden;" src="@Model[2].url" alt="Third slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>